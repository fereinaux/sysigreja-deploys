<body>

    <div class="wrapper" style="display:flex;flex-direction:column">
        <div class="row-eventos">
            <div class="col-xs-12 col-select" style="height:50px;margin-top:20px;display:flex;padding:0">
                <div class="div-cols" style="width:100%">
                    <div class="col-md-4 col-xs-12" style="display: flex">
                        <img class="logo-igreja" style="max-height: 3rem; max-width:6rem;min-width:3rem; margin-top: auto; margin-bottom: auto;" src="data:image/png;base64,@ViewBag.Configuracao.Logo" />

                        <h3 class="desktop" style="margin-top: auto; margin-bottom: auto; margin-left: 10px; font-weight: 900;font-size:20px ">@ViewBag.Action</h3>

                        @if (ViewBag.CountIgrejas > 0)
                        {
                            <div class="col-md-4 col-xs-12 select-igreja mobile" style="display: none;padding:0;align-items:center">
                                <i style="margin-left: auto; font-size: 2.5rem;margin-right:5px" class="fas fa-church"></i>


                                <select class="form-control" style="width: fit-content !important;  margin-top: auto; margin-bottom: auto; " id="igreja-mobile" onchange=" loadEventos()">
                                    <option value="">Selecione a igreja</option>
                                    @foreach (var igreja in ViewBag.Igrejas)
                                    {
                                        if (igreja == ViewBag.Configuracao.Identificador)
                                        {
                                            <option selected value="@igreja">@igreja</option>
                                        }
                                        else
                                        {
                                            <option value="@igreja">@igreja</option>

                                        }
                                    }
                                </select>

                            </div>
                        }



                    </div>

                    <div class="col-md-4 col-xs-12 pesquisa-evento" style="display: flex;align-items:center">

                        <input type="text" placeholder="Pesquisar eventos, congressos, acampamentos..." class="form-control" style="padding-left:30px;margin-top: auto; margin-bottom: auto;" name="search-evento" id="search-evento" value="" />
                        <i class="fas fa-search search-icon"></i>
                        <div class="container-calendar" style="cursor: pointer; margin-left: 10px; margin-top: auto;">

                            <i style="font-size: 30px;border:15px" class="far fa-calendar-alt"></i>

                            <div style="display:none" id="calendar">

                            </div>
                        </div>

                    </div>

                    @if (ViewBag.CountIgrejas > 0)
                    {
                        <div class="col-md-4 col-xs-12 select-igreja desktop" style="display: none;padding-left:0">

                            <h6 style=" margin-left:auto ;margin-top: auto; margin-bottom: auto; margin-right: 10px; font-weight: 700; font-size: 20px ">IGREJA</h6>
                            <select class="form-control" style="width: fit-content !important ; min-width: 200px;  margin-top: auto; margin-bottom: auto; " id="igreja-desktop" onchange=" loadEventos()">
                                <option value="">Selecione</option>
                                @foreach (var igreja in ViewBag.Igrejas)
                                {
                                    if (igreja == ViewBag.Configuracao.Identificador)
                                    {
                                        <option selected value="@igreja">@igreja</option>
                                    }
                                    else
                                    {
                                        <option value="@igreja">@igreja</option>

                                    }
                                }
                            </select>

                        </div>
                    }
                    @*else
                    {
                        <div class="col-md-4 col-xs-12 " style="display: flex">
                            <a style="margin-left: auto;" href="https://lp.sysigreja.com" class="btn btn-outline button-create">CRIE SEU EVENTO</a>
                        </div>
                    }*@



                </div>
            </div>
        </div>
        <div class="css-slideshow row-eventos">
            <img id="ad1" onclick="window.open('https://amzn.to/4faCZFb','_blank')" class="view" src="~/assets/img/ad/1.png"  />
            <img id="ad2" onclick="window.open('https://amzn.to/48nsM5E','_blank')" class="d-none" src="~/assets/img/ad/2.png"  />
            <img id="ad3" onclick="window.open('https://amzn.to/3ND0eeE','_blank')" class="d-none" src="~/assets/img/ad/3.png"  />
            <img id="ad4" onclick="window.open('https://amzn.to/3NzQEJF','_blank')" class="d-none" src="~/assets/img/ad/4.png"  />
        </div>

        <div class="row row-eventos" id="row-eventos" style="">

        </div>
    </div>

    <div id="preloader">
        <div class="loader" id="loader-1"></div>
    </div>
</body>

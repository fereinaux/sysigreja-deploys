@{
    ViewBag.Numero = new Random().Next();
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12 p-md">
        <h2>@ViewBag.Title</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="/Home/Index">Home</a>
            </li>
            <li class="breadcrumb-item active">
                <strong>@ViewBag.Title</strong>
            </li>
        </ol>
    </div>
    <div class="col-xl-12 col-md-1">
        <div class="radio i-checks-green"><label> <input type="radio" class="tipo" value="1" checked="" name="etiquetas-tipo"> <i></i> Participante </label></div>
        <div class="radio i-checks-green"><label> <input type="radio" class="tipo" value="2"  name="etiquetas-tipo"> <i></i> Equipante </label></div>
    </div>
    <div class="col-xl-12 col-md-1">
        <div class="radio i-checks-green"><label> <input type="radio" class="cracha"  value="1" checked="" name="cracha-tipo"> <i></i> Crachá </label></div>
        <div class="radio i-checks-green"><label> <input type="radio" class="cracha" value="2" name="cracha-tipo"> <i></i> Etiqueta </label></div>
        <div class="radio i-checks-green"><label> <input type="radio" class="cracha" value="3"  name="cracha-tipo"> <i></i> Tag </label></div>
    </div>
    <div class="col-xs-12 col-md-2">
        <h5>Evento</h5>
        <select class="form-control chosen-select" id="etiquetas-eventoid" onchange="GetAdjacentes()">
            @foreach (var evento in ViewBag.Eventos)
            {
                <option value="@evento.Id">@evento.TipoEvento @evento.Numeracao.ToString()</option>
            }
        </select>
    </div>
    <div id="participante">
        <div class="col-xs-12 col-md-4">
            <h5>Participante</h5>
            <select class="form-control chosen-select" id="etiquetas-participantes"></select>
        </div>
        <div class="col-xs-12 col-md-4">
            <h5>Círculo</h5>
            <select class="form-control chosen-select" id="etiquetas-circulos" onchange="LimparCampos()"></select>
        </div>
    </div>
    <div id="equipante" class="d-none">
        <div class="col-xs-12 col-md-4">
            <h5>Equipante</h5>
            <select class="form-control chosen-select" id="etiquetas-equipantes"></select>
        </div>
        <div class="col-xs-12 col-md-4">
            <h5>Equipe</h5>
            <select class="form-control chosen-select" id="etiquetas-equipes" onchange="LimparCampos()"></select>
        </div>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight etiqueta">
    <div class="ibox">
        <div class="ibox-content">
            <div class="row p-xs">
                <div class="col-xs-12 col-lg-6 col-lg-offset-3">
                    <div class="col-xs-12 p-xs">
                        <button class="btn btn-default form-control" id="btn_Imprimir" onclick="CarregarEtiquetas()">Imprimir Todas</button>
                    </div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(1) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(2) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(3) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(4) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(5) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(6) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(7) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(8) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(9) "><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(10)"><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(11)"><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(12)"><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(13)"><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                    <div class="col-xs-6 p-xs" onclick="CarregarEtiquetaIndividual(14)"><div class="pointer" style="height:70px;width:100%;background-color:#d1dce3;"></div></div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {

    <script src="~/Scripts/app/Participante/etiquetas.js?@ViewBag.Numero"></script>
    <script src="~/Images/Montserrat-normal.js" type="module"></script>
    <script src="~/Images/mont-normal.js" type="module"></script>

    <script>

    </script>
}

<script>
    setInterval(() => {
        const oldId = Number($('.css-slideshow img.view')[0].id.substring(2, 3))

        const newId = oldId == 4 ? 1 : oldId + 1

        $(`#ad${oldId}`).removeClass("view")
        $(`#ad${newId}`).addClass("view")

        $(`#ad${newId}`).removeClass("d-none")
        $(`#ad${oldId}`).addClass("d-none")
        
    },
    7000)
</script>

